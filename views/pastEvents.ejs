<% include ./partials/header %>

  
    <div class="container">
    
      
         <%events.forEach(function(event){%>
        
         
        
        
            <p style="background-color:#ccc;
                   font-family: 'Nunito', sans-serif;
                   font-size:20px;
                   margin-top:15px;
                   padding:15px"><%=event.EventName%>
           <span style="font-family: 'Nunito', sans-serif;
                    font-size:15px;
                    color:rgba(0, 51, 153,0.7);
                    
                    padding:4px;
                    border-radius:5px"><%=event.EventDateInWords%></span>
           <a href="/open/<%=event._id%>"
              style="float:right;
                             background-color:rgb(0, 51, 153);
                             color:white;
                             padding:6px;
                             font-size:15px;
                             border-radius:5px
                             ">Register <i class="fa fa-arrow-circle-right"></i></a>
         </p>
         <%})%>
      
      
    </div>
  
</body>





