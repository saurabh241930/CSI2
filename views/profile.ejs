  <link href="https://fonts.googleapis.com/css?family=Playfair+Display" rel="stylesheet"> 


  

</head>
<body style="max-width: 100%;
    overflow-x: hidden;">
  
  
 

  
   <nav class="navbar navbar-inverse navbar-fixed-top" style="background-color:#003399;font-family: 'Nunito', sans-serif;border-bottom:solid 1px #003399">
  <div class="container-fluid">
    <div class="navbar-header">
       <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>                        
            </button>
      <a style="color:white" class="navbar-brand" href="/">CSI</a>
    </div>
    <div class="collapse navbar-collapse" id="myNavbar">
      <ul class="nav navbar-nav">
        <li><a href="/allEvents"><strong style="background-color:#ccc;padding:5px;color:#003994">All Events</strong></a></li>
        <li><a href="/pastEvents"><strong style="background-color:#ccc;padding:5px;color:#003994">Past Events</strong></a></li>
        <li><a href="/gallery"><strong style="background-color:#ccc;padding:5px;color:#003994">Gallery</strong></a></li>
        <li><a href="/contact"><strong style="background-color:#ccc;padding:5px;color:#003994">Contact Us</strong></a></li>
         
        </ul>
         <ul class="nav navbar-nav navbar-right ">
           <%if(currentUser){%>
           <%if(currentUser.username === "Admin"){%>
        <li><a href="/AdminPortal"><strong style="background-color:#ccc;padding:5px;color:#003994">Admin Side</strong></a></li>
        <li><a href="/AddEvent"><strong style="background-color:#ccc;padding:5px;color:#003994">Add Events</strong></a></li>
        <li><a href="/uploadImage"><strong style="background-color:#ccc;padding:5px;color:#003994">Add Image</strong></a></li>
            
           <%}%>
        <%}%>
         <% if(!currentUser){%>
                   <li><a class=" mr-2" type="button"  data-toggle="modal" data-target=".bd-example-modal-lg">Register</a></li>
             <% }else{ %>
        <li><a class=" mr-2" href="/profile/<%=currentUser._id%>"><strong style="background-color:#ccc;padding:5px;color:#003994"><%= currentUser.username%></strong></a></li>
                  <li><a class=" mr-2" type="button" href="/logout">
                     <span style="background-color:white;padding:5px;color:black">Log Out</span></a></li>
        
              <% } %>
           
      </ul>
    </div> 
  </div>
</nav> 
 

  
<div class="row">
   <div class="col col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12"
        style="background-color:#003399">
     <div class="col col-xl-2 col-lg-2 col-md-12 col-sm-12 col-xs-12">
          <p style="font-family: 'Nunito', sans-serif;
                   font-size:100px;
                   margin-top:50px"><strong></strong>
            
     
       </p>
     </div>
     <div class="col col-xl-8 col-lg-8 col-md-12 col-sm-12 col-xs-12"
          style="text-align:center">
         <p class="pg" style="font-family: 'Nunito', sans-serif;
                   font-size:80px;
                   color:white;
                   margin-top:50px;"><strong class="pg">Profile</strong>
            
     
       </p>
     </div>
     <div class="col col-xl-2 col-lg-2 col-md-12 col-sm-12 col-xs-12">
      
     </div>
     
    </div>
  <div class="col col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12" 
         style="height:50px;background-color:rgba(0,0,0,0.9)">
  
  <div style="text-align:center" class="col col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12" >
        <div style='margin-top:10px'><a href="/" style="font-family: 'Nunito', sans-serif;font-size:25px;
                                color:white;
                                text-align:center"><i class="fa fa-user"></i> Home Page</a></div>
      </div>
    </div>
  
  
  
    <div class="col col-xl-12 col-lg-12 col-md-12 col-sm-12 col-xs-12">
      
     <p style="font-size:30px"><span style="font-family: 'Nunito', sans-serif;">Student Name : </span><%=user.StudentName%></p> 
      <p style="font-size:30px"><span style="font-family: 'Nunito', sans-serif;">PID : </span><%=user.username%></p>
      <p style="font-size:30px"><span style="font-family: 'Nunito', sans-serif;">Class : </span><%=user.Class%></p>
      <p style="font-size:30px"><span style="font-family: 'Nunito', sans-serif;">RollNumber : </span><%=user.RollNumber%></p>
      <p style="font-size:30px"><span style="font-family: 'Nunito', sans-serif;">MobileNumber : </span><%=user.MobileNumber%></p>
      <br>
      <p style="font-size:30px;
                text-align:center;"><span style="font-family: 'Nunito', sans-serif;"><span style="background-color:lightblue;
                                                                                                  color:black;">RegisteredEvents:- </span></p>
      <% user.RegisteredEvents.forEach(function(event){%>
        <p style="font-size:30px;
                  background-color:#ccc;
                  text-align:center;"><span style="font-family: 'Nunito', sans-serif;"></span><%=event.EventName%></p>
      <br>
      <% }) %>
     </div>
  
  
  
  
  
</div>

<script>
 if($(window).width() < 767){
      $("p").css("font-size","20px")
         
    }

</script>









